{
  "services": [
    {
      "name": "app",
      "image": "",
      "target_num_containers": 1,
      "sequential_deployment": true,
      "autorestart": "ALWAYS",
      "autoredeploy": true,
      "container_envvars": [
        {
          "key": "DATABASE_URL",
          "value": "postgres://postgres:PNzOlkeO9a5WuzZoqRdooA@db:5432/postgres"
        }
      ],
      "linked_to_service": [
        {
          "name": "db",
          "to_service": "db"
        }
      ],
      "container_ports": [
        {
          "protocol": "tcp",
          "inner_port": 3000,
          "published": true,
          "outer_port": 6506
        }
      ]
    },
    {
      "name": "db",
      "image": "postgres",
      "target_num_containers": 1,
      "sequential_deployment": true,
      "container_ports": [
        {
          "protocol": "tcp",
          "inner_port": 5432,
          "published": true
        }
      ],
      "container_envvars": [
        {
          "key": "POSTGRES_PASSWORD",
          "value": "PNzOlkeO9a5WuzZoqRdooA"
        }
      ]
    }
  ],
  "name": "cparrattomartini-example"
}